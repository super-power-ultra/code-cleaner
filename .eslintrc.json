{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-plusplus": "off",
    "linebreak-style": "off",
    "no-console": "off",
    "no-alert": "off"
  }
}
